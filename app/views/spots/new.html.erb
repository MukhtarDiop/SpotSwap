<div class="container">
    <h1>New Spot</h1>
    <%= simple_form_for @spot do |f| %>
        <%= f.input :description %>
        <%= f.input :category %>
        <%= f.input :lat %>
        <%= f.input :long %>
        <%= f.input :length %>
        <%= f.input :width %>
        <%= f.input :height %>
        <%= f.input :rate %>
        <%= f.submit %>
    <% end %>
</div>